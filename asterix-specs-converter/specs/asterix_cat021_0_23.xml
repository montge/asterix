                            <BitsName>Trajectory Intent Data</BitsName>
                            <BitsPresence>2</BitsPresence>
                        </Bits>
                        <Bits bit="6">
                            <BitsShortName>spare</BitsShortName>
                            <BitsName>Spare bits set to 0</BitsName>
                        </Bits>
                        <Bits bit="5">
                            <BitsShortName>spare</BitsShortName>
                            <BitsName>Spare bits set to 0</BitsName>
                        </Bits>
                        <Bits bit="4">
                            <BitsShortName>spare</BitsShortName>
                            <BitsName>Spare bits set to 0</BitsName>
                        </Bits>
