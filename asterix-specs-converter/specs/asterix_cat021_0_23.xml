        <DataItemFormat desc="2-octet fixed length data item.">
            <Fixed length="2">
                <Bits from="16" to="1" encode="signed">
                    <BitsShortName>BVR</BitsShortName>
                    <BitsName>Barometric Vertical Rate</BitsName>
                    <BitsUnit scale="6.25">ft/min</BitsUnit>
                </Bits>
            </Fixed>
        </DataItemFormat>
    </DataItem>

    <DataItem id="157">
        <DataItemName>Geometric Vertical Rate</DataItemName>
        <DataItemDefinition>
            Geometric Vertical Rate, in two&apos;s complement form, with reference to WGS-84.
        </DataItemDefinition>
        <DataItemFormat desc="2-octet fixed length data item.">
            <Fixed length="2">
                <Bits from="16" to="1" encode="signed">
                    <BitsShortName>GVR</BitsShortName>
                    <BitsName>Geometric Vertical Rate</BitsName>
                    <BitsUnit scale="6.25">ft/min</BitsUnit>
                </Bits>
            </Fixed>
        </DataItemFormat>
    </DataItem>

    <DataItem id="160">
        <DataItemName>Ground Vector</DataItemName>
        <DataItemDefinition>
            Ground Speed and Track Angle elements of Ground Vector.
        </DataItemDefinition>
        <DataItemFormat desc="4-octet fixed length data item.">
            <Fixed length="4">
                <Bits from="32" to="17" encode="signed">
                    <BitsShortName>GS</BitsShortName>
                    <BitsName>Ground Speed in Two&apos;s Complement Form Referenced to WGS84</BitsName>
                    <BitsUnit scale="0.00006103515625" min="0" max="2">NM/s</BitsUnit>
                </Bits>
                <Bits from="16" to="1" encode="unsigned">
                    <BitsShortName>TA</BitsShortName>
                    <BitsName>Track Angle</BitsName>
                    <BitsUnit scale="0.0054931640625">deg</BitsUnit>
                </Bits>
            </Fixed>
        </DataItemFormat>
    </DataItem>

    <DataItem id="165">
        <DataItemName>Rate Of Turn</DataItemName>
        <DataItemDefinition>
            Rate of Turn, in two&apos;s complement form.
        </DataItemDefinition>
        <DataItemFormat desc="Variable data item.">
            <Variable>
                <Fixed length="1">
                    <Bits from="8" to="7">
                        <BitsShortName>TI</BitsShortName>
                        <BitsName>Turn Indicator</BitsName>
                        <BitsValue val="0">Not available</BitsValue>
                        <BitsValue val="1">Left</BitsValue>
                        <BitsValue val="2">Right</BitsValue>
                        <BitsValue val="3">Straight</BitsValue>
                    </Bits>
                    <Bits from="6" to="2">
                        <BitsShortName>spare</BitsShortName>
                        <BitsName>Spare bit(s) set to 0</BitsName>
                        <BitsConst>0</BitsConst>
                    </Bits>
                    <Bits bit="1" fx="1">
                        <BitsShortName>FX</BitsShortName>
                        <BitsName>Extension Indicator</BitsName>
                        <BitsValue val="0">End of Data Item</BitsValue>
                        <BitsValue val="1">Extension</BitsValue>
                    </Bits>
                </Fixed>
                <Fixed length="1">
                    <Bits from="8" to="2" encode="signed">
                        <BitsShortName>ROT</BitsShortName>
                        <BitsName>Rate of Turn</BitsName>
                        <BitsUnit scale="0.25" max="15">deg/s</BitsUnit>
                    </Bits>
                    <Bits bit="1" fx="1">
                        <BitsShortName>FX</BitsShortName>
                        <BitsName>Extension Indicator</BitsName>
                        <BitsValue val="0">End of Data Item</BitsValue>
                        <BitsValue val="1">Extension</BitsValue>
                    </Bits>
                </Fixed>
                <Fixed length="1">
                    <Bits bit="1" fx="1">
                        <BitsShortName>FX</BitsShortName>
                        <BitsName>Extension Indicator</BitsName>
                        <BitsValue val="0">End of Data Item</BitsValue>
                        <BitsValue val="1">Extension</BitsValue>
                    </Bits>
                </Fixed>
            </Variable>
        </DataItemFormat>
        <DataItemNote>
            Notes:
                1. A positive value represents a right turn, whereas a negative value
                   represents a left turn.
                2. Value 15 means 15 deg/s or above.
        </DataItemNote>
    </DataItem>

    <DataItem id="170">
        <DataItemName>Target Identification</DataItemName>
        <DataItemDefinition>
            Target (aircraft or vehicle) identification in 8 characters, as reported
            by the target.
        </DataItemDefinition>
        <DataItemFormat desc="6-octet fixed length data item.">
            <Fixed length="6">
                <Bits from="48" to="1" encode="6bitschar">
                    <BitsShortName>TI</BitsShortName>
                    <BitsName>Target Identification</BitsName>
                </Bits>
            </Fixed>
        </DataItemFormat>
    </DataItem>

    <DataItem id="200">
        <DataItemName>Target Status</DataItemName>
        <DataItemDefinition>
            Status of the target
        </DataItemDefinition>
        <DataItemFormat desc="1-octet fixed length data item.">
            <Fixed length="1">
                <Bits from="8" to="1">
                    <BitsShortName>TS</BitsShortName>
                    <BitsName>Target Status</BitsName>
                    <BitsValue val="0">No emergency / not reported</BitsValue>
                    <BitsValue val="1">General emergency</BitsValue>
                    <BitsValue val="2">Lifeguard / medical</BitsValue>
                    <BitsValue val="3">Minimum fuel</BitsValue>
                    <BitsValue val="4">No communications</BitsValue>
                    <BitsValue val="5">Unlawful interference</BitsValue>
                </Bits>
            </Fixed>
        </DataItemFormat>
    </DataItem>

    <DataItem id="210">
        <DataItemName>Link Technology Indicator</DataItemName>
        <DataItemDefinition>
            Indication of which ADS link technology has been used to send the target
            report.
        </DataItemDefinition>
        <DataItemFormat desc="1-octet fixed length data item.">
            <Fixed length="1">
                <Bits from="8" to="6">
                    <BitsShortName>spare</BitsShortName>
                    <BitsName>Spare bit(s) set to 0</BitsName>
                    <BitsConst>0</BitsConst>
                </Bits>
                <Bits bit="5">
                    <BitsShortName>DTI</BitsShortName>
                    <BitsName>Cockpit Display of Traffic Information</BitsName>
                    <BitsValue val="0">Unknown</BitsValue>
                    <BitsValue val="1">Aircraft equiped with CDTI</BitsValue>
                </Bits>
                <Bits bit="4">
                    <BitsShortName>MDS</BitsShortName>
                    <BitsName>Mode-S Extended Squitter</BitsName>
                    <BitsValue val="0">Not used</BitsValue>
                    <BitsValue val="1">Used</BitsValue>
                </Bits>
                <Bits bit="3">
                    <BitsShortName>UAT</BitsShortName>
                    <BitsName>UAT</BitsName>
                    <BitsValue val="0">Not used</BitsValue>
                    <BitsValue val="1">Used</BitsValue>
                </Bits>
                <Bits bit="2">
                    <BitsShortName>VDL</BitsShortName>
                    <BitsName>VDL Mode 4</BitsName>
                    <BitsValue val="0">Not used</BitsValue>
                    <BitsValue val="1">Used</BitsValue>
                </Bits>
                <Bits bit="1">
                    <BitsShortName>OTR</BitsShortName>
                    <BitsName>Other Technology</BitsName>
                    <BitsValue val="0">Not used</BitsValue>
                    <BitsValue val="1">Used</BitsValue>
                </Bits>
            </Fixed>
        </DataItemFormat>
    </DataItem>

    <DataItem id="220">
        <DataItemName>Met Information</DataItemName>
        <DataItemDefinition>
            Meteorological information.
        </DataItemDefinition>
        <DataItemFormat desc="Compound data item.">
            <Compound>
                <Variable>
                    <Fixed length="1">
                        <Bits bit="8">
                            <BitsShortName>WS</BitsShortName>
                            <BitsName>Wind Speed</BitsName>
                            <BitsPresence>1</BitsPresence>
                        </Bits>
                        <Bits bit="7">
                            <BitsShortName>WD</BitsShortName>
                            <BitsName>Wind Direction</BitsName>
                            <BitsPresence>2</BitsPresence>
                        </Bits>
                        <Bits bit="6">
                            <BitsShortName>TMP</BitsShortName>
                            <BitsName>Temperature</BitsName>
                            <BitsPresence>3</BitsPresence>
                        </Bits>
                        <Bits bit="5">
                            <BitsShortName>TRB</BitsShortName>
                            <BitsName>Turbulence</BitsName>
                            <BitsPresence>4</BitsPresence>
                        </Bits>
                        <Bits bit="4">
                            <BitsShortName>spare</BitsShortName>
                            <BitsName>Spare bits set to 0</BitsName>
                        </Bits>
                        <Bits bit="3">
                            <BitsShortName>spare</BitsShortName>
                            <BitsName>Spare bits set to 0</BitsName>
                        </Bits>
                        <Bits bit="2">
                            <BitsShortName>spare</BitsShortName>
                            <BitsName>Spare bits set to 0</BitsName>
                        </Bits>
                        <Bits bit="1" fx="1">
                            <BitsShortName>FX</BitsShortName>
                            <BitsName>Extension indicator</BitsName>
                            <BitsValue val="0">no extension</BitsValue>
                            <BitsValue val="1">extension</BitsValue>
                        </Bits>
                    </Fixed>
                </Variable>

                <Fixed length="2">
                    <Bits from="16" to="1" encode="unsigned">
                        <BitsShortName>WS</BitsShortName>
                        <BitsName>Wind Speed</BitsName>
                        <BitsUnit scale="1.0" min="0" max="300">kt</BitsUnit>
                    </Bits>
                </Fixed>

                <Fixed length="2">
                    <Bits from="16" to="1" encode="unsigned">
                        <BitsShortName>WD</BitsShortName>
                        <BitsName>Wind Direction</BitsName>
                        <BitsUnit scale="1.0" min="1" max="360">deg</BitsUnit>
                    </Bits>
                </Fixed>

                <Fixed length="2">
                    <Bits from="16" to="1" encode="signed">
                        <BitsShortName>TMP</BitsShortName>
                        <BitsName>Temperature</BitsName>
                        <BitsUnit scale="0.25" min="-100" max="100">degC</BitsUnit>
                    </Bits>
                </Fixed>

                <Fixed length="1">
                    <Bits from="8" to="1" encode="unsigned">
                        <BitsShortName>TRB</BitsShortName>
                        <BitsName>Turbulence</BitsName>
                        <BitsUnit min="0" max="15"></BitsUnit>
                    </Bits>
                </Fixed>
            </Compound>
        </DataItemFormat>
    </DataItem>

    <DataItem id="230">
        <DataItemName>Roll Angle</DataItemName>
        <DataItemDefinition>
            The roll angle, in two&apos;s complement form, of an aircraft executing a turn.
        </DataItemDefinition>
        <DataItemFormat desc="2-octet fixed length data item.">
            <Fixed length="2">
                <Bits from="16" to="1" encode="signed">
                    <BitsShortName>RA</BitsShortName>
                    <BitsName>Roll Angle</BitsName>
                    <BitsUnit scale="1.0" min="-180" max="180">deg</BitsUnit>
                </Bits>
            </Fixed>
        </DataItemFormat>
        <DataItemNote>
            Notes:
                1. Negative Value indicates Left Wing Down.
                2. Resolution provided by the technology 1090 MHz Extended Squitter
                   is 1 degree.
        </DataItemNote>
    </DataItem>

    <DataItem id="RE">
        <DataItemName>Reserved Expansion Field</DataItemName>
        <DataItemDefinition>
