    <DataItem id="030">
        <DataItemName>Time of Day</DataItemName>
        <DataItemDefinition>
            Time of applicability (measurement) of the reported position, in the form of elapsed time since last midnight, expressed as UTC.
        </DataItemDefinition>
        <DataItemFormat desc="3-octet fixed length data item.">
            <Fixed length="3">
                <Bits from="24" to="1" encode="unsigned">
                    <BitsShortName>ToD</BitsShortName>
                    <BitsName>Time of Day</BitsName>
                    <BitsUnit scale="0.0078125">s</BitsUnit>
                </Bits>
            </Fixed>
        </DataItemFormat>
        <DataItemNote>
            The time of the day value is reset to zero at every midnight.
        </DataItemNote>
    </DataItem>

