<Category id="63" name="Sensor Status Reports" ver="1.7">

    <DataItem id="010">
        <DataItemName>Data Source Identifier</DataItemName>
        <DataItemDefinition>
            Identification of the radar station from which the data are received.
        </DataItemDefinition>
        <DataItemFormat desc="2-octet fixed length data item.">
            <Fixed length="2">
                <Bits from="16" to="9">
                    <BitsShortName>SAC</BitsShortName>
                    <BitsName>System Area Code</BitsName>
                </Bits>
                <Bits from="8" to="1">
                    <BitsShortName>SIC</BitsShortName>
                    <BitsName>System Identification Code</BitsName>
                </Bits>
            </Fixed>
        </DataItemFormat>
        <DataItemNote>
            Note:
                The defined SACs are on the EUROCONTROL ASTERIX website
                (www.eurocontrol.int/asterix)
        </DataItemNote>
    </DataItem>

    <DataItem id="015">
        <DataItemName>Service Identification</DataItemName>
        <DataItemDefinition>
            Identification of the service provided to one or more users.
        </DataItemDefinition>
        <DataItemFormat desc="1-octet fixed length data item.">
            <Fixed length="1">
                <Bits from="8" to="1">
                    <BitsShortName>SI</BitsShortName>
                    <BitsName>Service Identification</BitsName>
                </Bits>
            </Fixed>
        </DataItemFormat>
        <DataItemNote>
            The service identification is allocated by the SDPS
        </DataItemNote>
    </DataItem>

    <DataItem id="030">
        <DataItemName>Time of Message</DataItemName>
        <DataItemDefinition>
            Absolute time stamping of the message, in the form of elapsed time
            since last midnight, expressed as UTC.
        </DataItemDefinition>
        <DataItemFormat desc="3-octet fixed length data item.">
            <Fixed length="3">
                <Bits from="24" to="1" encode="unsigned">
                    <BitsShortName>ToM</BitsShortName>
                    <BitsName>Time of Message</BitsName>
                    <BitsUnit scale="0.0078125">s</BitsUnit>
                </Bits>
            </Fixed>
        </DataItemFormat>
        <DataItemNote>
            The time of the day value is reset to zero at every midnight.
        </DataItemNote>
    </DataItem>

    <DataItem id="050">
        <DataItemName>Sensor Identifier</DataItemName>
        <DataItemFormat desc="2-octet fixed length data item.">
            <Fixed length="2">
                <Bits from="16" to="9">
                    <BitsShortName>SAC</BitsShortName>
                    <BitsName>System Area Code</BitsName>
                </Bits>
                <Bits from="8" to="1">
                    <BitsShortName>SIC</BitsShortName>
                    <BitsName>System Identification Code</BitsName>
                </Bits>
            </Fixed>
        </DataItemFormat>
        <DataItemNote>
            - The up-to-date list of SACs is published on the EUROCONTROL
              Web Site (http://www.eurocontrol.int/asterix).
            - If the SAC/SIC refers to an SDPS used as input, the respective
              sensor status information will be transmitted using the Reserved
              Expansion Field.
        </DataItemNote>
    </DataItem>

    <DataItem id="060">
        <DataItemName>Sensor Configuration and Status</DataItemName>
        <DataItemDefinition>
            Configuration and status of the sensor
        </DataItemDefinition>
        <DataItemFormat desc="Variable data item.">
            <Variable>
                <Fixed length="1">
                    <Bits from="8" to="7">
                        <BitsShortName>CON</BitsShortName>
                        <BitsValue val="0">Operational</BitsValue>
                        <BitsValue val="1">Degraded</BitsValue>
                        <BitsValue val="2">Initialization</BitsValue>
                        <BitsValue val="3">Not currently connected</BitsValue>
                    </Bits>
                    <Bits bit="6">
                        <BitsShortName>PSR</BitsShortName>
                        <BitsValue val="0">PSR GO</BitsValue>
                        <BitsValue val="1">PSR NOGO</BitsValue>
                    </Bits>
                    <Bits bit="5">
                        <BitsShortName>SSR</BitsShortName>
                        <BitsValue val="0">SSR GO</BitsValue>
                        <BitsValue val="1">SSR NOGO</BitsValue>
                    </Bits>
                    <Bits bit="4">
                        <BitsShortName>MDS</BitsShortName>
                        <BitsValue val="0">MDS GO</BitsValue>
                        <BitsValue val="1">MDS NOGO</BitsValue>
                    </Bits>
                    <Bits bit="3">
                        <BitsShortName>ADS</BitsShortName>
                        <BitsValue val="0">ADS GO</BitsValue>
                        <BitsValue val="1">ADS NOGO</BitsValue>
                    </Bits>
                    <Bits bit="2">
                        <BitsShortName>MLT</BitsShortName>
                        <BitsValue val="0">MLT GO</BitsValue>
                        <BitsValue val="1">MLT NOGO</BitsValue>
                    </Bits>
                    <Bits bit="1" fx="1">
                        <BitsShortName>FX</BitsShortName>
                        <BitsName>Extension Indicator</BitsName>
                        <BitsValue val="0">End of Data Item</BitsValue>
                        <BitsValue val="1">Extension</BitsValue>
                    </Bits>
                </Fixed>
                <Fixed length="1">
                    <Bits bit="8">
                        <BitsShortName>OPS</BitsShortName>
                        <BitsName>Operational Release Status of the System</BitsName>
                        <BitsValue val="0">System is released for operational use</BitsValue>
                        <BitsValue val="1">Operational use of System is inhibited</BitsValue>
                    </Bits>
                    <Bits bit="7">
                        <BitsShortName>ODP</BitsShortName>
                        <BitsName>Data Processor Overload Indicator</BitsName>
                        <BitsValue val="0">Default, no overload</BitsValue>
                        <BitsValue val="1">Overload in DP</BitsValue>
                    </Bits>
                    <Bits bit="6">
                        <BitsShortName>OXT</BitsShortName>
                        <BitsName>Transmission Subsystem Overload Status</BitsName>
                        <BitsValue val="0">Default, no overload</BitsValue>
                        <BitsValue val="1">Overload in transmission subsystem</BitsValue>
                    </Bits>
                    <Bits bit="5">
                        <BitsShortName>MSC</BitsShortName>
                        <BitsName>Monitoring System Connected Status</BitsName>
                        <BitsValue val="0">Monitoring system connected</BitsValue>
                        <BitsValue val="1">Monitoring system disconnected</BitsValue>
                    </Bits>
                    <Bits bit="4">
                        <BitsShortName>TSV</BitsShortName>
                        <BitsName>Time Source Validity</BitsName>
                        <BitsValue val="0">Valid</BitsValue>
                        <BitsValue val="1">Invalid</BitsValue>
                    </Bits>
                    <Bits bit="3">
                        <BitsShortName>NPW</BitsShortName>
                        <BitsName>No Plot Warning</BitsName>
                        <BitsValue val="0">Default (no meaning)</BitsValue>
                        <BitsValue val="1">No plots being received</BitsValue>
                    </Bits>
                    <Bits bit="2">
                        <BitsShortName>spare</BitsShortName>
                        <BitsName>Spare bit(s) set to 0</BitsName>
                        <BitsConst>0</BitsConst>
                    </Bits>
                    <Bits bit="1" fx="1">
                        <BitsShortName>FX</BitsShortName>
                        <BitsName>Extension Indicator</BitsName>
                        <BitsValue val="0">End of Data Item</BitsValue>
                        <BitsValue val="1">Extension</BitsValue>
                    </Bits>
                </Fixed>
                <Fixed length="1">
                    <Bits from="8" to="7">
                        <BitsShortName>TTF</BitsShortName>
                        <BitsName>Test Target Failure Status from Sensor</BitsName>
                    </Bits>
                    <Bits from="6" to="5">
                        <BitsShortName>SPO</BitsShortName>
                        <BitsName>Indication of Spoofing Attack from Sensor</BitsName>
                    </Bits>
                    <Bits from="4" to="2">
                        <BitsShortName>spare</BitsShortName>
                        <BitsName>Spare bit(s) set to 0</BitsName>
                        <BitsConst>0</BitsConst>
                    </Bits>
                    <Bits bit="1" fx="1">
                        <BitsShortName>FX</BitsShortName>
                        <BitsName>Extension Indicator</BitsName>
                        <BitsValue val="0">End of Data Item</BitsValue>
                        <BitsValue val="1">Extension</BitsValue>
                    </Bits>
                </Fixed>
                <Fixed length="1">
                    <Bits bit="1" fx="1">
                        <BitsShortName>FX</BitsShortName>
                        <BitsName>Extension Indicator</BitsName>
                        <BitsValue val="0">End of Data Item</BitsValue>
                        <BitsValue val="1">Extension</BitsValue>
                    </Bits>
                </Fixed>
            </Variable>
        </DataItemFormat>
        <DataItemNote>
            Notes:

                1. The information (CON) is derived by the SDPS, is implementation dependent
                   and shall be described in the ICD.
                2. The information (PSR), (SSR), (MDS), (ADS) and (MLT) and well as (OPS),
                   (ODP), (OXT), (MSC), (TSV), (SPO) and (TTF) are derived from the
                   monosensor service message categories and are only relevant for
                   operational sensors (i.e. CON = 0).
                3. The sensor status derived from the SDPS indicating the use of a specific
                   technology is provided in I063/REF/SSFS/PSR, I063/REF/SSFS/SSR,
                   I063/REF/SSFS/MDS, I063/REF/SSFS/ADS and I063/REF/SSFS/MLT.
                4. This legacy I063/060/NPW No Plot Warning might be either sensor or
                   SDPS derived. A SDPS derived No Plot Warning is available in
                   I063/REF/SSS/NPWS.
        </DataItemNote>
    </DataItem>

    <DataItem id="070">
        <DataItemName>Time Stamping Bias</DataItemName>
        <DataItemDefinition>
            Plot Time stamping bias, in two&apos;s complement form
        </DataItemDefinition>
        <DataItemFormat desc="2-octet fixed length data item.">
            <Fixed length="2">
                <Bits from="16" to="1" encode="signed">
                    <BitsShortName>TSB</BitsShortName>
                    <BitsName>Time Stamping Bias</BitsName>
                    <BitsUnit scale="1.0">ms</BitsUnit>
                </Bits>
            </Fixed>
        </DataItemFormat>
    </DataItem>

    <DataItem id="080">
        <DataItemName>SSR / Mode S Range Gain and Bias</DataItemName>
        <DataItemDefinition>
            SSR / Mode S Range Gain and Range Bias, in two&apos;s complement form.
        </DataItemDefinition>
        <DataItemFormat desc="4-octet fixed length data item.">
            <Fixed length="4">
                <Bits from="32" to="17" encode="signed">
                    <BitsShortName>SRG</BitsShortName>
                    <BitsName>Mode S Range Gain</BitsName>
                    <BitsUnit scale="1.0"></BitsUnit>
                </Bits>
                <Bits from="16" to="1" encode="signed">
                    <BitsShortName>SRB</BitsShortName>
                    <BitsName>Mode S Range Bias</BitsName>
                    <BitsUnit scale="0.0078125">NM</BitsUnit>
                </Bits>
            </Fixed>
        </DataItemFormat>
        <DataItemNote>
            Note:

                The following formula is used to correct range:

                .. math::

                    \rho_\mathrm{corrected} = \frac{\rho_\mathrm{measured} - range\_bias}{1 + range\_gain}
        </DataItemNote>
    </DataItem>

    <DataItem id="081">
        <DataItemName>SSR Mode S Azimuth Bias</DataItemName>
        <DataItemDefinition>
            SSR / Mode S Azimuth Bias, in two&apos;s complement form.
        </DataItemDefinition>
        <DataItemFormat desc="2-octet fixed length data item.">
            <Fixed length="2">
                <Bits from="16" to="1" encode="signed">
                    <BitsShortName>SMSAB</BitsShortName>
                    <BitsName>SSR Mode S Azimuth Bias</BitsName>
                    <BitsUnit scale="0.0054931640625">deg</BitsUnit>
                </Bits>
            </Fixed>
        </DataItemFormat>
        <DataItemNote>
            Note:

                The following formula is used to correct azimuth:

                .. math::

                    \theta_\mathrm{corrected} = \theta_\mathrm{measured} - azimuth\_bias
        </DataItemNote>
    </DataItem>

